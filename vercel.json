{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "crons": [
    {
      "path": "/api/schedule-ledger-notifications",
      "schedule": "0 2 * * *"
    },
    {
      "path": "/api/process-ledger-notifications",
      "schedule": "0 10 * * *"
    },
    {
      "path": "/api/process-campaign-messages",
      "schedule": "*/1 * * * *"
    }
  ],
  "functions": {
  },
  "rewrites": [
  ]
} 